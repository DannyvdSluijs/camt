    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "globalcitizen/php-iban": "^2.6",
        "moneyphp/money": "^3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Genkgo\\Camt\\": ["src"]
        }
